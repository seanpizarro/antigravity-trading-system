# .env.example - DUAL ACCOUNT CONFIGURATION TEMPLATE
# Copy this file to .env and fill in your actual credentials

# LIVE ACCOUNT (Real Money)
TASTYTRADE_LIVE_REFRESH_TOKEN=your_live_refresh_token_here
TASTYTRADE_LIVE_CLIENT_ID=your_live_client_id_here
TASTYTRADE_LIVE_CLIENT_SECRET=your_live_client_secret_here
TASTYTRADE_LIVE_ACCOUNT_NUMBER=your_live_account_number_here

# PAPER ACCOUNT (Sandbox)
TASTYTRADE_PAPER_REFRESH_TOKEN=your_paper_refresh_token_here
TASTYTRADE_PAPER_CLIENT_ID=your_paper_client_id_here
TASTYTRADE_PAPER_CLIENT_SECRET=your_paper_client_secret_here
TASTYTRADE_PAPER_ACCOUNT_NUMBER=your_paper_account_number_here

# TRADING MODE
TRADING_MODE=both  # paper, live, or both

# DeepSeek API Key
DEEPSEEK_API_KEY=your_deepseek_api_key_here
